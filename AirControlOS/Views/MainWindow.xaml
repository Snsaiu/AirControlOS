                        <ColumnDefinition Width="23*"/>
                        <ColumnDefinition Width="24*"/>
                        <ColumnDefinition Width="7*"/>
                        <ColumnDefinition Width="35*"/>
                        <ColumnDefinition Width="11*"/>
                        <ColumnDefinition Width="22*"/>
                        <ColumnDefinition Width="23*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="11*"/>
                        <RowDefinition Height="29*"/>
                        <RowDefinition Height="9*"/>
                        <RowDefinition Height="22*"/>
                        <RowDefinition Height="18*"/>
                        <RowDefinition Height="78*"/>
                    </Grid.RowDefinitions>
                    <Border BorderBrush="#FFE8E8E8" BorderThickness="1" Grid.ColumnSpan="7" Background="#FF5F5F5F" Grid.RowSpan="5" CornerRadius="10"/>


                    <Button x:Name="button3" Command="{Binding DriveState}" Content="干燥辅热" Margin="4,-0.934,4.4,2.934"  Style="{DynamicResource ButtonStyle1}" Grid.Row="1" Grid.ColumnSpan="2" d:LayoutOverrides="TopMargin, BottomMargin"/>
                    <Button x:Name="button5" Command="{Binding VerWindSweepMode}" Content="上下扫风" Margin="4,1.053,4.4,2" Grid.Row="3" Style="{DynamicResource ButtonStyle2}" d:LayoutOverrides="LeftMargin, RightMargin, TopMargin, BottomMargin" Grid.ColumnSpan="2"/>
                    <Button x:Name="button3_Copy" Command="{Binding TemperatureDisplayMode}" Content="温度显示" Margin="-3.013,-0.934,-4.055,2.934"  Style="{DynamicResource ButtonStyle1}" Grid.Row="1" Grid.Column="3" d:LayoutOverrides="LeftMargin, RightMargin, TopMargin, BottomMargin"/>
                    <Button x:Name="button3_Copy1" Command="{Binding HeathAndAirChangeMode}" Content="健康换气" Margin="3.816,0,2.513,2"  Style="{DynamicResource ButtonStyle1}" Grid.Row="1" Grid.Column="5" Grid.ColumnSpan="2" d:LayoutOverrides="TopMargin, BottomMargin"/>
                    <Button x:Name="button5_Copy" Command="{Binding HorWindSweepMode}" Content="左右扫风" Margin="3.816,1.053,2.513,2" Grid.Row="3" Style="{DynamicResource ButtonStyle2}" Grid.Column="5" Grid.ColumnSpan="2" d:LayoutOverrides="LeftMargin, RightMargin, TopMargin, BottomMargin"/>
                    <Button x:Name="button5_Copy1" Content="定时" Command="{Binding TimerMode}" Grid.Row="4" Style="{DynamicResource ButtonStyle2}" FontSize="10" Grid.ColumnSpan="2" Grid.Column="1" Margin="0,3,0,2.5" d:LayoutOverrides="LeftMargin, RightMargin"/>
                    <Button x:Name="button5_Copy2" Content="睡眠" Command="{Binding SleepMode}" Grid.Row="4" Style="{DynamicResource ButtonStyle2}" FontSize="10" Grid.ColumnSpan="2" Grid.Column="4" Margin="0,3,0,2.5" d:LayoutOverrides="LeftMargin, RightMargin"/>
                    <Border BorderBrush="#FF6A6A6A" BorderThickness="1" Grid.Column="3" Grid.Row="2" Grid.RowSpan="2" Margin="-3.013,0,-5.722,0" CornerRadius="5">
                        <Border.Background>
                            <ImageBrush ImageSource="fantasy_worldlogo.jpg"/>
                        </Border.Background>
                    </Border>
                </Grid>
                <Button x:Name="button7" Content="模式" Grid.Column="2" Grid.Row="1" Command="{Binding WorkModel}" Style="{DynamicResource ButtonStyle1}" Margin="0,4" d:LayoutOverrides="LeftMargin, RightMargin, TopMargin, BottomMargin"/>


            </Grid>
            <Border BorderThickness="3,5,3,0" Grid.Column="1" Grid.Row="1" Margin="30,0,30,0" CornerRadius="5">
                <Border.Background>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0,0">
                        <GradientStop Color="#FFAAAAAA" Offset="0"/>
                        <GradientStop Color="#FFCBCBCB" Offset="1"/>
                        <GradientStop Color="#FFF9F9F9" Offset="0.45"/>
                        <GradientStop Color="#FFCBCBCB" Offset="0.134"/>
                        <GradientStop Color="#FFEAEAEA" Offset="0.773"/>
                    </LinearGradientBrush>
                </Border.Background>
                <Border.BorderBrush>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0,0">
