                    <Button x:Name="button1" Command="{Binding AddTemperature}" Content="▲" Style="{DynamicResource ButtonStyle2}"  FontSize="20" Margin="6.652,5.667,5.334,3" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Grid.Column="1" />
                    <Button x:Name="button2" Command="{Binding SubTemperature}" Content="▼" Style="{DynamicResource ButtonStyle2}"  FontSize="20" Margin="6.652,3,5.334,6.25" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Grid.Row="1" Grid.Column="1" />
                    <Button x:Name="button8" Content="超强" Style="{DynamicResource ButtonStyle1}" Margin="0,5.667,6.334,2" Command="{Binding SuperPowerMode}"/>
                    <Button x:Name="button8_Copy" Command="{Binding LightMode}" Content="灯光" Style="{DynamicResource ButtonStyle1}" Margin="5,5.667,0,3" Grid.Column="2"/>
                    <Button x:Name="button8_Copy1" Command="{Binding SilenceMode}" Content="静音" Style="{DynamicResource ButtonStyle1}" Margin="0,3,6.334,4.667" Grid.Row="1"/>
                    <Button x:Name="button8_Copy2" Content="节能" Style="{DynamicResource ButtonStyle1}" Margin="5,3,0,4.667" Grid.Row="1" Grid.Column="2"/>
                </Grid>
                <Grid Grid.ColumnSpan="3" Grid.Column="1" Grid.Row="3" d:LayoutOverrides="LeftMargin, RightMargin, TopMargin, BottomMargin" Grid.RowSpan="2">
