        </Grid>
        <Expander x:Name="expander" Grid.Column="3" d:LayoutOverrides="LeftMargin, RightMargin, TopMargin, BottomMargin" Margin="0,0,0.001,0" Grid.RowSpan="2">
            <StackPanel>
                <Button Content="Login" Command="{Binding OpenloginWindowCommand}" x:Name="LoginBtn" Background="#FF2E2E2E" Foreground="White"/>
                <Button Content="-" x:Name="MinBtn"  Background="#FF2E2E2E" Foreground="White" FontWeight="Bold" FontFamily="Algerian" Command="{Binding MinSizeCommand}" CommandParameter="{Binding ElementName=Main}"/>
                <Button Content="X" Command="{Binding CloseMainWindowCommand}" CommandParameter="{Binding ElementName= Main}" x:Name="offBtn" Background="#FF2E2E2E" Foreground="White"/>

                <Button Content="list" Command="{Binding OpenListWindowCommand}" x:Name="ListBtn" Background="#FF2E2E2E" Foreground="White"/>
            </StackPanel>
        </Expander>

    </Grid>
</Window>
