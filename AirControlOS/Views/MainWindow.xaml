                                <TextBlock x:Name="textBlock1" Grid.Column="2" TextWrapping="Wrap" Text="▧▧▧▧" FontSize="10" VerticalAlignment="Center" HorizontalAlignment="Right"/>
                            </Grid>
                        </Border>
                        <Border BorderBrush="Black" BorderThickness="0,0,0,1" Grid.Column="1" Grid.Row="1" d:LayoutOverrides="LeftMargin, RightMargin, TopMargin, BottomMargin">
                            <Border.Resources>

                                <AssistCompnentResourecs:TemperatureAddSymbol x:Key="TemperatureAddSymbol"></AssistCompnentResourecs:TemperatureAddSymbol>
                            </Border.Resources>
                            <TextBlock x:Name="TemperatureView" TextWrapping="Wrap" Text="{Binding TemperatureDisplay}" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="50"/>
                        </Border>
                        <Border BorderBrush="Black" BorderThickness="0,1,0,0" Grid.Column="1" Grid.Row="3" d:LayoutOverrides="LeftMargin, RightMargin, TopMargin, BottomMargin">
