                        <GradientStop Color="Gray" Offset="0.186"/>
                        <GradientStop Color="#FF707070" Offset="0.349"/>
                        <GradientStop Color="#FFD1D1D1" Offset="0.506"/>
                        <GradientStop Color="#FF666666" Offset="0.673"/>
                        <GradientStop Color="#FF9D9D9D" Offset="0.829"/>
