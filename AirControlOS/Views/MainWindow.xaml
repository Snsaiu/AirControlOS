                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <TextBlock x:Name="HorWindView" TextWrapping="Wrap" Text="{Binding HorWindView}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                <TextBlock x:Name="VerWindView" TextWrapping="Wrap" Text="{Binding VerWindView}" VerticalAlignment="Center" Grid.Column="1" HorizontalAlignment="Center"/>
                            </Grid>
                        </Border>
                        <Border BorderBrush="Black" BorderThickness="0" Grid.ColumnSpan="2" Grid.Row="2" d:LayoutOverrides="LeftMargin, RightMargin, TopMargin, BottomMargin">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition/>
                                    <ColumnDefinition />
                                    <ColumnDefinition />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <TextBlock x:Name="SuperPowerView" Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Center" TextWrapping="Wrap" Text="{Binding SuperPowerView}" Background="#FF0E0E0E" Foreground="#FF1B5908" OpacityMask="White" FontWeight="Bold" Grid.Row="1" Height="19" Margin="0,1" />
                                <TextBlock x:Name="DryView" Grid.Column="1" HorizontalAlignment="Center" TextWrapping="Wrap" Text="{Binding DryView}" Background="#FF0E0E0E" Foreground="#FF265D05" Margin="0,1"/>
                                <TextBlock x:Name="HealthView" Grid.Column="2" TextWrapping="Wrap" Text="{Binding HealthView}" Background="#FF0E0E0E" Foreground="#FF1B5908" OpacityMask="White" FontWeight="Bold" HorizontalAlignment="Center" Margin="0,1"  />
                                <TextBlock x:Name="AirChangeView" Grid.Column="3" TextWrapping="Wrap" Text="{Binding AirChangeView}" Background="#FF0E0E0E" Foreground="#FF1B5908" OpacityMask="White" FontWeight="Bold" HorizontalAlignment="Center" Margin="0,1" />
                                <TextBlock x:Name="AutoSlieceView" TextWrapping="Wrap" Text="{Binding AutoSlieceView}" Background="#FF0E0E0E" Foreground="#FF265D05" Grid.Row="1" HorizontalAlignment="Right" Margin="0,1,0.5,1"/>
                                <TextBlock x:Name="SlienceView" TextWrapping="Wrap" Text="{Binding SlienceView}" Background="#FF0E0E0E" Foreground="#FF265D05" Margin="0.5,1,0,1" Grid.Row="1" Grid.Column="1" HorizontalAlignment="Left"/>
                                <TextBlock x:Name="SleepView" Grid.Column="3" TextWrapping="Wrap" Text="{Binding SleepView}" Background="#FF0E0E0E" Foreground="#FF1B5908" OpacityMask="White" FontWeight="Bold" HorizontalAlignment="Center"  Grid.Row="1" Margin="0,1" />
                            </Grid>
                        </Border>
                    </Grid>
                </Border>
            </Grid>
            <Grid x:Name="grid" Grid.Column="1" Grid.Row="3" d:LayoutOverrides="LeftMargin, RightMargin, TopMargin, BottomMargin">
