                </Border.BorderBrush>
                <TextBlock x:Name="textBlock" HorizontalAlignment="Center" VerticalAlignment="Center" TextWrapping="Wrap" Text="FANTASY WORLD" Foreground="Black" TextDecorations="{x:Null}" FontFamily="Adobe Caslon Pro Bold" FontWeight="Bold" FontSize="16" FontStyle="Italic" TextOptions.TextHintingMode="Animated">
                    <TextBlock.Effect>
                        <ee:EmbossedEffect Color="#FF1B1B1B" Height="0.000900000000009"/>
                    </TextBlock.Effect>
                </TextBlock>
